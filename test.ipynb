    "def settle_total_bet(df, bet_amount):\n",
    "    outcome = df['outcome']\n",
    "    price = df['price']\n",
    "    if outcome == \"won\":\n",
    "        return bet_amount * (price - 1)\n",
    "    else:\n",
    "        return bet_amount * -1\n"
   ]
  },
